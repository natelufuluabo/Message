<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <div>
      <% messages.forEach((message) => { %>
        <p><%= message.text %></p>
        <p><%= message.user %></p>
        <p><%= message.added %></p>
      <% }); %>
      <a href="/new">Add Message</a>
    </div>
  </body>
</html>
